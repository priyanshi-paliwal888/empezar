<odoo>
    <record id="move_in_out_invoice_report_action" model="ir.actions.report">
        <field name="name">Move In/Move Out Invoice Report</field>
        <field name="model">move.in.out.invoice</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">empezar_account_invoices.report_move_in_out_invoice_document</field>
        <field name="report_file">empezar_account_invoices.report_move_in_out_invoice_document</field>
        <field name="print_report_name">(object._get_report_filename())</field>
    </record>
     <record id="credit_note_invoice_report_action" model="ir.actions.report">
