        <field name="report_type">qweb-pdf</field>
        <field name="report_name">empezar_account_invoices.report_credit_note_invoice_document</field>
        <field name="report_file">empezar_account_invoices.report_credit_note_invoice_document</field>
        <field name="print_report_name">(object._get_report_filename())</field>
    </record>
</odoo>
